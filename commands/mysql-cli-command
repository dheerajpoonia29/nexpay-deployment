$ sudo /usr/local/mysql-commercial-9.3.0-macos15-arm64/support-files/mysql.server start

$ mysql -u root -p
// ask password = root1234

$ sudo /usr/local/mysql-commercial-9.3.0-macos15-arm64/support-files/mysql.server stop

mysql> show databases;
mysql> create database nexapay;
mysql> use nexapay;
mysql> show tables;
mysql> select * from bank_table;


// list all mysql instance running
$ ps aux | grep mysqld
$ // kill mysql instance
sudo kill -9 4222 4306
